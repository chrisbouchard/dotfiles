URxvt*loginShell: false
URxvt*meta: ignore
URxvt*utmpInhibit: true
URxvt*urgentOnBell: true

# urxvt*perl-lib: /home/chris/git/urxvt-vim-scrollback
# urxvt*perl-ext-common: vim-scrollback

URxvt*geometry: 120x40

URxvt*saveLines: 12000
URxvt*scrollBar: false
URxvt*scrollstyle: plain
URxvt*secondaryScroll: true
Urxvt*urlLauncher: google-chrome

URxvt*font: xft:terminus:pixelsize=8:antialias=true:hinting=true
URxvt*boldFont: xft:terminus:bold:pixelsize=8:antialias=true:hinting=true
URxvt*italicFont: xft:terminus:italic:pixelsize=8:antialias=true:hinting=true
URxvt*boldItalicFont: xft:terminus:bold:italic:pixelsize=8:antialias=true:hinting=true

URxvt*transparent: true
URxvt*shading: 10
URxvt*background: #000000
URxvt*foreground: gray90
URxvt*color7: gray90
URxvt*colorBD: #ffffff
URxvt*cursorColor: #e0e080
URxvt*throughColor: #8080f0
URxvt*highlightColor: #f0f0f0

# urxvt.vim-scrollback: C-v
# urxvt.vim-scrollback-paste: C-r

dzen2*font: -*-terminus-*-r-*-*-9-*-*-*-*-*-*-*
dzen2*background: #000000
dzen2*foreground: black

